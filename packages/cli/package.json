{
  "name": "openportal",
  "version": "0.1.15",
  "description": "CLI to run OpenCode Portal - a web UI for OpenCode AI coding agent",
  "type": "module",
  "bin": {
    "openportal": "bin/openportal.js"
  },
  "files": [
    "bin",
    "dist",
    "web"
  ],
  "scripts": {
    "build": "npx tsx scripts/build.ts",
    "dev": "npx tsx src/cli.ts",
    "typecheck": "tsc --noEmit",
    "release": "npx tsx scripts/publish.ts",
    "release:patch": "npx tsx scripts/publish.ts patch",
    "release:minor": "npx tsx scripts/publish.ts minor",
    "release:major": "npx tsx scripts/publish.ts major"
  },
  "dependencies": {
    "commander": "^13.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "opencode",
    "ai",
    "coding",
    "assistant",
    "portal",
    "web-ui",
    "cli"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hosenur/portal.git"
  },
  "homepage": "https://github.com/hosenur/portal#readme",
  "bugs": {
    "url": "https://github.com/hosenur/portal/issues"
  }
}
